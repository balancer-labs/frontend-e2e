{
  "baseUrl": "http://localhost:8080",
  "userAgent": "synpress",
  "retries": { "runMode": 1, "openMode": 1 },
  "integrationFolder": "synpress/specs",
  "screenshotsFolder": "synpress/screenshots",
  "videosFolder": "synpress/videos",
  "chromeWebSecurity": true,
  "viewportWidth": 1366,
  "viewportHeight": 768,
  "component": {
    "componentFolder": ".",
    "testFiles": "**/*spec.{js,jsx,ts,tsx}"
  },
  "env": {
    "coverage": false
  },
  "defaultCommandTimeout": 40000,
  "pageLoadTimeout": 40000,
  "requestTimeout": 40000,
  "supportFile": "synpress/support/index.ts",
  "reporter": "mochawesome",
  "reporterOptions": {
    "reportDir": "synpress/report",
    "embeddedScreenshots": true,
    "html": false,
    "json": true,
    "showSkipped": true
  }
}
